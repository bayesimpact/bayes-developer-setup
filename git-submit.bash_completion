#!bash
#
# bash completion support for git-submit
#
# (LINUX) Copy this file in /etc/bash_completion.d/git-submit
# (MacOs) Copy this file in $(brew --prefix)/etc/bash_completion.d/git-submit

_git_submit()
{
  __gitcomp "$(__git_heads)"
}

[[ "$OSTYPE" == "darwin"* ]] && complete -F _git_submit git-submit
